#include <iostream>

using namespace std;

void swap(int& numberOne, int& numberTwo)
{
	numberOne = numberOne + numberTwo;
	numberTwo = numberOne - numberTwo;
	numberOne = numberOne - numberTwo;
	
}

int main()
{
	int a = 5, b = 8;
	cout << "a = " << a << ", b = " << b << endl;

	swap(a, b);

	cout << "a = " << a << ", b = " << b << endl;
}

